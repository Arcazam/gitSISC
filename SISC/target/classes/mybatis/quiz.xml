<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="quiz">
		<insert id="insertQuiz">
			insert into study_memo
			values(qz_seq.nextval,#{que_cate},#{question},#{answer},#{interpre})
		</insert>
		
		<select id="getJavaQuiz" resultType="SiscQuiz">
			select * from quiz where que_cate = 'JAVA' order by qz_num asc
		</select>
		
	</mapper>